<ul class="pagination">
  <li class="<%= pageToPresent == 1 ? 'disabled' : '' %>"><a
      href="<%= habitationsPageUrlPrefix + (+pageToPresent - (pageToPresent == 1 ? 0 : 1)) %>">&laquo;</a></li>
  <% for (var i = 0; i < +lastPage; i++) { %>
  <li class="<%= pageToPresent == i + 1 ? 'active' : '' %>"><a
      href="<%= habitationsPageUrlPrefix %> <%= i + 1 %>"><%= i + 1 %></a></li>
  <% } %>
  <li class="<%= pageToPresent == lastPage ? 'disabled' : '' %>"><a
      href="<%= habitationsPageUrlPrefix + (+pageToPresent + (pageToPresent == lastPage ? 0 : 1)) %>">&raquo;</a>
  </li>
</ul>
