<div class="row">
  <% var ucols = ["col-md-12", "col-md-6", "col-md-4", "col-md-3"] %>
  <% users.forEach(function(usr){ %>
  <div class=<%= ucols[users.length - 1] %>>
    <h3><%= usr.name %></h3>
    <a href="<%= usr.link %>" target="_blank"><img
        src="<%= gravatar.url(usr.emailPersonel, {s: 220, d: 'mm', r: 'g'}) %>" alt="<%= usr.name %>"
        title="<%= usr.name %>" class="img-circle"/></a>

    <p><a class="btn" href="<%= usr.link %>" target="_blank">More »</a></p>
    <table class="table table-striped table-bordered">
      <thead>
      </thead>
      <tbody>
      <tr>
        <td>Nome</td>
        <td><%= usr.fullName %></td>
      </tr>
      <tr>
        <td>Número</td>
        <td><a href="http://thoth.cc.e.ipl.pt/profile/student/<%= usr.nr %>" target="_blank"><%= usr.nr %></a></td>
      </tr>
      <tr>
        <td>e-mail ISEL</td>
        <td><a href="mailto:<%= usr.emailIsel %>" target="_blank"><%= usr.emailIsel %></a></td>
      </tr>
      <tr>
        <td>e-mail IPL</td>
        <td><a href="mailto:<%= usr.emailIpl %>" target="_blank"><%= usr.emailIpl %></a></td>
      </tr>
      <tr>
        <td>e-mail privado</td>
        <td><a href="mailto:<%= usr.emailPersonel %>" target="_blank"><%= usr.emailPersonel %></a></td>
      </tr>
      </tbody>
    </table>
  </div>
  <% }); %>
</div>
